{
  "author": "17hoehbr <17hoehbr@gmail.com>",
  "homepage": "https://github.com/17hoehbr/Apple-Music-Wrapper",
  "name": "apple-music",
  "productName": "apple-music",
  "version": "1.1.0",
  "description": "Electron wrapper for Apple Music",
  "license": "MIT",
  "buildResources": "assets",
  "scripts": {
    "init": "install",
    "start": "electron --disable-gpu .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "electron": "https://github.com/castlabs/electron-releases#v8.5.3-wvvmp",
    "electron-builder": "^22.7.0",
    "electron-webpack": "^2.8.2",
    "webpack": "~4.42.1"
  },
  "build": {
    "electronDownload": {
      "mirror": "https://github.com/castlabs/electron-releases/releases/download/v"
    },
    "appId": "apple-music",
    "extends": null,
    "files": [
      "**/*",
      "assets/icon.*"
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "snap",
        "rpm"
      ],
      "category": "AudioVideo",
      "icon": "./assets/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./assets/icon.ico"
    }
  },
  "main": "index.js",
  "repository": "git@github.com:17hoehbr/Apple-Music-Wrapper.git"
}
